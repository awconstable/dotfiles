export PATH="/usr/local/sbin:$PATH"

alias df='df -h'
alias du='du -h'

alias grep='grep --color'
alias egrep='egrep --color'
alias fgrep='fgrep --color'

alias ls='ls -hFG'
alias ll='ls -lA'
alias la='ls -AG'

export HOMESHICK_DIR=/usr/local/opt/homeshick
source "/usr/local/opt/homeshick/homeshick.sh"

